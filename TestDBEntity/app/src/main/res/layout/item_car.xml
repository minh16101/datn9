                        android:src="@drawable/comfortable_chair_svgrepo_com"
                        android:layout_margin="10dp"/>
                </LinearLayout>
                <ImageView
                    android:id="@+id/seat_13"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/comfortable_chair_svgrepo_com"
                    app:layout_constraintTop_toBottomOf="@id/linear_car_layout_2"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintBottom_toTopOf="@+id/linear_car_layout_3"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginRight="20dp"/>
                <LinearLayout
                    android:id="@+id/linear_car_layout_3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="6"
                    app:layout_constraintTop_toBottomOf="@+id/seat_13"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent">
                    <ImageView
                        android:id="@+id/seat_14"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/comfortable_chair_svgrepo_com"
                        android:layout_margin="10dp"/>
                    <ImageView
                        android:id="@+id/seat_15"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/comfortable_chair_svgrepo_com"
                        android:layout_margin="10dp"/>
                    <ImageView
                        android:id="@+id/seat_16"
                        android:layout_width="0dp"
