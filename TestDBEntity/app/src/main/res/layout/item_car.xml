                />
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="5">
                <LinearLayout
                    android:id="@+id/linear_car_layout_4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="6"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent">
                    <ImageView
                        android:id="@+id/seat_20"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/comfortable_chair_svgrepo_com"
                        android:layout_margin="10dp"/>
                    <ImageView
                        android:id="@+id/seat_21"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/comfortable_chair_svgrepo_com"
                        android:layout_margin="10dp"/>
                    <ImageView
                        android:id="@+id/seat_22"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/comfortable_chair_svgrepo_com"
                        android:layout_margin="10dp"/>
                    <ImageView
                        android:id="@+id/seat_23"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/comfortable_chair_svgrepo_com"
                        android:layout_margin="10dp"/>
                    <ImageView
                        android:id="@+id/seat_24"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/comfortable_chair_svgrepo_com"
                        android:layout_margin="10dp"/>
                    <ImageView
                        android:id="@+id/seat_25"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/comfortable_chair_svgrepo_com"
                        android:layout_margin="10dp"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linear_car_layout_5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="6"
                    app:layout_constraintTop_toBottomOf="@+id/linear_car_layout_4"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent">
                    <ImageView
                        android:id="@+id/seat_26"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/comfortable_chair_svgrepo_com"
                        android:layout_margin="10dp"/>
                    <ImageView
                        android:id="@+id/seat_27"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/comfortable_chair_svgrepo_com"
                        android:layout_margin="10dp"/>
                    <ImageView
                        android:id="@+id/seat_28"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/comfortable_chair_svgrepo_com"
                        android:layout_margin="10dp"/>
                    <ImageView
                        android:id="@+id/seat_29"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/comfortable_chair_svgrepo_com"
                        android:layout_margin="10dp"/>
                    <ImageView
                        android:id="@+id/seat_30"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/comfortable_chair_svgrepo_com"
                        android:layout_margin="10dp"/>
                    <ImageView
                        android:id="@+id/seat_31"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/comfortable_chair_svgrepo_com"
                        android:layout_margin="10dp"/>
                </LinearLayout>
                <ImageView
                    android:id="@+id/seat_32"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/comfortable_chair_svgrepo_com"
                    app:layout_constraintTop_toBottomOf="@id/linear_car_layout_5"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintBottom_toTopOf="@+id/linear_car_layout_6"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginRight="20dp"/>
                <LinearLayout
                    android:id="@+id/linear_car_layout_6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="6"
                    app:layout_constraintTop_toBottomOf="@+id/seat_32"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent">
                    <ImageView
                        android:id="@+id/seat_33"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/comfortable_chair_svgrepo_com"
                        android:layout_margin="10dp"/>
                    <ImageView
                        android:id="@+id/seat_34"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/comfortable_chair_svgrepo_com"
                        android:layout_margin="10dp"/>
                    <ImageView
                        android:id="@+id/seat_35"
                        android:layout_width="0dp"
