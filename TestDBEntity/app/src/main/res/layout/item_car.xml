                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/comfortable_chair_svgrepo_com"
                        android:layout_margin="10dp"/>
                    <ImageView
                        android:id="@+id/seat_37"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/comfortable_chair_svgrepo_com"
                        android:layout_margin="10dp"/>
                    <ImageView
                        android:id="@+id/seat_38"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/comfortable_chair_svgrepo_com"
                        android:layout_margin="10dp"/>
                </LinearLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="5dp"
                android:layout_weight="2">
                <ImageView
                    android:id="@+id/seat_not_book"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:src="@drawable/comfortable_chair_svgrepo_com"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintRight_toLeftOf="@+id/tv_not_book"
                    android:layout_margin="10dp"
                    app:layout_constraintLeft_toLeftOf="parent"/>
                <TextView
                    android:id="@+id/tv_not_book"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toRightOf="@+id/seat_not_book"
                    app:layout_constraintRight_toRightOf="@+id/seat_book"
                    android:text="Not Booking"
                    android:gravity="center"
                    android:textSize="20sp"
                    android:textColor="@color/black"
                    android:layout_margin="10dp"
                    />
                <ImageView
                    android:id="@+id/seat_book"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:src="@drawable/confortable_chair_booked"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:layout_margin="10dp"
                    app:layout_constraintLeft_toRightOf="@id/tv_not_book"
                    app:layout_constraintRight_toLeftOf="@+id/tv_book"/>
                <TextView
                    android:id="@+id/tv_book"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toRightOf="@+id/seat_book"
                    app:layout_constraintRight_toRightOf="parent"
                    android:text="Booking"
                    android:gravity="center"
                    android:textSize="20sp"
                    android:textColor="@color/black"
                    android:layout_margin="10dp"
                    />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>