        app:layout_constraintRight_toRightOf="parent"
        xmlns:app="http://schemas.android.com/apk/res-auto">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:weightSum="14">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:textColor="@color/black"
                android:text="Floor 1"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="3dp"
                android:textStyle="bold"
                android:textSize="20sp"
                />
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="5">
                <LinearLayout
                    android:id="@+id/linear_car_layout_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="6"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent">
                    <ImageView
                        android:id="@+id/seat_1"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/comfortable_chair_svgrepo_com"
                        android:layout_margin="10dp"/>
                    <ImageView
                        android:id="@+id/seat_2"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/comfortable_chair_svgrepo_com"
                        android:layout_margin="10dp"/>
                    <ImageView
                        android:id="@+id/seat_3"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/comfortable_chair_svgrepo_com"
                        android:layout_margin="10dp"/>
                    <ImageView
                        android:id="@+id/seat_4"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/comfortable_chair_svgrepo_com"
                        android:layout_margin="10dp"/>
                    <ImageView
                        android:id="@+id/seat_5"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/comfortable_chair_svgrepo_com"
                        android:layout_margin="10dp"/>
                    <ImageView
                        android:id="@+id/seat_6"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/comfortable_chair_svgrepo_com"
                        android:layout_margin="10dp"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linear_car_layout_2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="6"
                    app:layout_constraintTop_toBottomOf="@+id/linear_car_layout_1"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent">
                    <ImageView
                        android:id="@+id/seat_7"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/comfortable_chair_svgrepo_com"
                        android:layout_margin="10dp"/>
                    <ImageView
                        android:id="@+id/seat_8"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/comfortable_chair_svgrepo_com"
                        android:layout_margin="10dp"/>
                    <ImageView
                        android:id="@+id/seat_9"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/comfortable_chair_svgrepo_com"
                        android:layout_margin="10dp"/>
                    <ImageView
                        android:id="@+id/seat_10"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:src="@drawable/comfortable_chair_svgrepo_com"
                        android:layout_margin="10dp"/>
                    <ImageView
                        android:id="@+id/seat_11"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
